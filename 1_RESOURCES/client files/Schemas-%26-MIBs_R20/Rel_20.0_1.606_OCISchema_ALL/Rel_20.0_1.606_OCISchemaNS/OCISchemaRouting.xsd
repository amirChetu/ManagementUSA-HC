  <xs:simpleType name="DigitManipulationSpecification">
    <xs:annotation>
      <xs:documentation>
        Digit Manipulation String obeying the following syntax:
        Format:
          Command1(operand1), Command2(operand2), ..., CommandN(operandN)
          where N is between 0 and 8 
        Commands:
          Cursor position   : END(),  MOV(y), POS(x),
          Digits removal    : DEL(x), LTR(s), RTR(s), TRM(s),
          Digits insertion  : APP(s), INS(s), PRE(s),
          Digits replacement: OVW(s), REP(s),
          Use other DMI     : DMI(x),
       Arguments:
          x : positive integer, y : integer, s : string
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="80"/>
    </xs:restriction>
  </xs:simpleType>    
    
