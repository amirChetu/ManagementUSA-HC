      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="ne" type="RoutingNE"/>
          <xs:element name="location" type="DN" minOccurs="0"/>
          <xs:element name="cost" type="ResourceCost" minOccurs="0"/>
          <xs:element name="weight" type="ResourceWeight" minOccurs="0"/>
          <xs:element name="profile" type="RoutingProfile" minOccurs="0"/>
          <xs:element name="poll" type="xs:boolean" minOccurs="0"/>
          <xs:element name="state" type="DeviceState" minOccurs="0"/>
          <xs:element name="signaling" type="RoutingNESignalingAttribute" minOccurs="0" maxOccurs="unbounded" nillable="true"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEAddressAddRequest">
    <xs:annotation>
      <xs:documentation>
        Add a Routing NE Address.
        If usePort is set to false, the port specified is not used and is reset to 0.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="ne" type="RoutingNE"/>
          <xs:element name="address" type="DeviceAddress"/>
          <xs:element name="cost" type="ResourceCost"/>
          <xs:element name="weight" type="ResourceWeight"/>
          <xs:element name="port" type="RoutingNEAddressPort"/>
          <xs:element name="usePort" type="xs:boolean"/>
          <xs:element name="transport" type="RoutingNEAddressTransportType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEAddressDeleteRequest">
    <xs:annotation>
      <xs:documentation>
