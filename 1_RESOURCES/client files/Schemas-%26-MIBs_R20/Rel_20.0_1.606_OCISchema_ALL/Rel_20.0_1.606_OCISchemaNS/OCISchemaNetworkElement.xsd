  <xs:complexType name="RoutingNEAddRequest">
    <xs:annotation>
      <xs:documentation>
        Add a Routing NE.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="ne" type="RoutingNE"/>
          <xs:element name="location" type="DN"/>
          <xs:element name="cost" type="ResourceCost"/>
          <xs:element name="weight" type="ResourceWeight"/>
          <xs:element name="profile" type="RoutingProfile"/>
          <xs:element name="poll" type="xs:boolean"/>
          <xs:element name="state" type="DeviceState"/>
          <xs:element name="signaling" type="RoutingNESignalingAttribute" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEDeleteRequest">
    <xs:annotation>
      <xs:documentation>
        Delete a Routing NE.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="ne" type="RoutingNE"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEGetListRequest">
    <xs:annotation>
      <xs:documentation>
        Get list of Routing NEs.
        The response is either RoutingNeGetListResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest"/>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEGetListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to RoutingNeGetListRequest.
        The column headings are: "Routing NE", "Location", "Cost", "Weight",
        "Profile", "Poll", "State", "Operational State", "Signaling Attributes".
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="routingNEListTable" type="core:OCITable"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEGetRequest">
    <xs:annotation>
      <xs:documentation>
        Get properties of a Routing NE.
        The response is either RoutingNeGetResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="ne" type="RoutingNE"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEGetResponse">
    <xs:annotation>
      <xs:documentation>
