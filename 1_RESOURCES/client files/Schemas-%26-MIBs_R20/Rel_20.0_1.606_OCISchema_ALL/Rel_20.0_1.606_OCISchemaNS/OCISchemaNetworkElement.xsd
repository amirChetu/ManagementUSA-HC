      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="location" type="DN"/>
          <xs:element name="cost" type="ResourceCost"/>
          <xs:element name="weight" type="ResourceWeight"/>
          <xs:element name="profile" type="RoutingProfile"/>
          <xs:element name="poll" type="xs:boolean"/>
          <xs:element name="state" type="DeviceState"/>
          <xs:element name="signaling" type="RoutingNESignalingAttribute" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEModifyRequest">
    <xs:annotation>
      <xs:documentation>
        Modify a Routing NE.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
