  </xs:complexType>    
    
  <xs:complexType name="RoutingNEMediaGetListResponse20">
    <xs:annotation>
      <xs:documentation>
        Response to RoutingNEMediaGetListRequest20.
        The column headings are: "Media Codec", "Priority".
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="codecBasedRtg" type="xs:boolean"/>
          <xs:element name="mediaCodec" type="MediaCodec" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEMediaClearRequest">
