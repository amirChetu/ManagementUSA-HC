          <xs:element name="codecBasedRtg" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEMediaGetSupportedMediaListRequest">
    <xs:annotation>
      <xs:documentation>
        Get the list of supported Routing NE Media codecs.
        The response is either RoutingNEMediaGetSupportedMediaListResponse or ErrorResponse.
        Replaced by: RoutingNEMediaGetSupportedMediaListRequest20
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest"/>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingNEMediaGetSupportedMediaListResponse">
