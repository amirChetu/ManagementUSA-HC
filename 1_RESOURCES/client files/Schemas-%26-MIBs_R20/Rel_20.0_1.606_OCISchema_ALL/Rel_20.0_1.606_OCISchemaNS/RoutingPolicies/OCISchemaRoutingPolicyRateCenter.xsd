      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="searchCriteriaExactCallType" type="SearchCriteriaExactCallType" minOccurs="0"/>
          <xs:element name="searchCriteriaExactRateCenterName" type="SearchCriteriaExactRateCenterName" minOccurs="0"/>
          <xs:element name="searchCriteriaExactCountryCode" type="SearchCriteriaExactCountryCode" minOccurs="0"/>
          <xs:element name="searchCriteriaExactZoneName" type="SearchCriteriaExactZoneName" minOccurs="0"/>
          <xs:element name="searchCriteriaExactAddress" type="SearchCriteriaExactDn" minOccurs="0"/>
          <xs:element name="searchCriteriaExactRoutingNE" type="SearchCriteriaExactRoutingNE" minOccurs="0"/>
          <xs:element name="searchCriteriaExactResourceCost" type="SearchCriteriaExactResourceCost" minOccurs="0"/>
          <xs:element name="searchCriteriaExactResourceWeight" type="SearchCriteriaExactResourceWeight" minOccurs="0"/>
          <xs:element name="searchCriteriaExactDigitManipulationIndex" type="SearchCriteriaExactDigitManipulationIndex" minOccurs="0"/>
          <xs:element name="searchCriteriaExactDestinationTrunkGroup" type="SearchCriteriaExactDestinationTrunkGroup" minOccurs="0"/>
          <xs:element name="undefinedRateCenter" type="xs:boolean" minOccurs="0"/>
          <xs:element name="responsePagingRestriction" type="ResponsePagingRestriction" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyRateCenterGetZoneListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to RoutingPolicyRateCenterGetZoneListRequest.
        The column headings are: "Call Type", "Country Code", "Zone", "Rate Center", "Address", "Routing Network Element",
        "Cost", "Weight", "Digit Manipulation Index", "Destination Trunk Group".
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="zoneTable" type="core:OCITable"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyRateCenterModifyZoneRequest">
    <xs:annotation>
      <xs:documentation>
        Request to modify a zone entry.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="callType" type="CallType"/>
          <xs:element name="countryCode" type="CountryCode"/>
          <xs:element name="zone" type="ZoneName"/>
          <xs:element name="rateCenter" type="RateCenterName"/>
          <xs:element name="destination" type="RateCenterRouteDestination"/>
          <xs:element name="cost" type="ResourceCost" minOccurs="0"/>
          <xs:element name="weight" type="ResourceWeight" minOccurs="0"/>
          <xs:element name="digitManipulationIndex" type="DigitManipulationIndex" minOccurs="0" nillable="true"/>
          <xs:element name="destinationTrunkGroup" type="DestinationTrunkGroup" minOccurs="0" nillable="true"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RateCenterRouteDestination">
    <xs:annotation>
      <xs:documentation>
        Represents the destination of the routing list entry.
      </xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="address" type="DN"/>
      <xs:element name="routingNE" type="RoutingNE"/>
    </xs:choice>
  </xs:complexType>    
    
</xs:schema>
