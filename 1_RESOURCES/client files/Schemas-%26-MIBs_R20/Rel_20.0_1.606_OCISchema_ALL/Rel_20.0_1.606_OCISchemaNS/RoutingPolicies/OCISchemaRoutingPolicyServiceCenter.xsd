          <xs:element name="gatewayAddress" type="GatewayAddress"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterGetTrunkGroupEntryResponse">
    <xs:annotation>
      <xs:documentation>
        Response to RoutingPolicyServiceCenterGetTrunkGroupEntryRequest.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="controllerGroupId" type="ControllerGroupId" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterGetTrunkGroupListRequest">
    <xs:annotation>
      <xs:documentation>
        Get list of trunk group entries.
        The response is either a RoutingPolicyServiceCenterGetTrunkGroupListResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="searchCriteriaExactTrunkGroupProtocol" type="SearchCriteriaExactTrunkGroupProtocol" minOccurs="0"/>
          <xs:element name="searchCriteriaExactTrunkGroupName" type="SearchCriteriaExactTrunkGroupName" minOccurs="0"/>
          <xs:element name="searchCriteriaExactChannelName" type="SearchCriteriaExactChannelName" minOccurs="0"/>
          <xs:element name="searchCriteriaExactGatewayAddress" type="SearchCriteriaExactGatewayAddress" minOccurs="0"/>
          <xs:element name="searchCriteriaExactControllerGroupId" type="SearchCriteriaExactControllerGroupId" minOccurs="0"/>
          <xs:element name="responsePagingRestriction" type="ResponsePagingRestriction" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterGetTrunkGroupListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to RoutingPolicyServiceCenterGetTrunkGroupListRequest.
        The column headings are : "Protocol", "Trunk Group", "Channel", "Gateway Address", "Controller Group Id".
      </xs:documentation>
    </xs:annotation>
