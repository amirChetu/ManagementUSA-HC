    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterModifyTrunkGroupRequest">
    <xs:annotation>
      <xs:documentation>
        Request to modify a trunk group entry.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="protocol" type="TrunkGroupProtocol"/>
          <xs:element name="trunkGroup" type="TrunkGroupName"/>
          <xs:element name="channel" type="ChannelName"/>
          <xs:element name="gatewayAddress" type="GatewayAddress"/>
          <xs:element name="controllerGroupId" type="ControllerGroupId" minOccurs="0" nillable="true"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:simpleType name="ChannelName">
    <xs:annotation>
      <xs:documentation>
        Channel name.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="15"/>
    </xs:restriction>
  </xs:simpleType>    
    
  <xs:simpleType name="GatewayAddress">
    <xs:annotation>
      <xs:documentation>
        Gateway address.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="255"/>
    </xs:restriction>
  </xs:simpleType>    
    
  <xs:simpleType name="ControllerGroupId">
    <xs:annotation>
      <xs:documentation>
        The group id of a gateway controller.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="10"/>
    </xs:restriction>
  </xs:simpleType>    
    
  <xs:simpleType name="TrunkGroupProtocol">
    <xs:annotation>
      <xs:documentation>
        Trunk group protocol.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="MGCP"/>
    </xs:restriction>
  </xs:simpleType>    
    
  <xs:complexType name="SearchCriteriaExactChannelName">
    <xs:annotation>
      <xs:documentation>
        Criteria for searching for a particular ChannelName.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SearchCriteria">
        <xs:sequence>
          <xs:element name="channelName" type="ChannelName"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="SearchCriteriaExactGatewayAddress">
    <xs:annotation>
      <xs:documentation>
        Criteria for searching for a particular GatewayAddress.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SearchCriteria">
        <xs:sequence>
          <xs:element name="gatewayAddress" type="GatewayAddress"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="SearchCriteriaExactControllerGroupId">
    <xs:annotation>
      <xs:documentation>
        Criteria for searching for a particular ControllerGroupId.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SearchCriteria">
        <xs:sequence>
          <xs:element name="controllerGroupId" type="ControllerGroupId"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="SearchCriteriaExactTrunkGroupProtocol">
    <xs:annotation>
      <xs:documentation>
        Criteria for searching for a particular TrunkGroupProtocol.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="SearchCriteria">
        <xs:sequence>
          <xs:element name="trunkGroupProtocol" type="TrunkGroupProtocol"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="ServiceCenterRouteDestination">
    <xs:annotation>
      <xs:documentation>
        Represents the destination of the routing list entry.
      </xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="address" type="DN"/>
      <xs:element name="routingNE" type="RoutingNE"/>
      <xs:element name="trunkGroupId" type="NetworkTrunkGroupId"/>
    </xs:choice>
  </xs:complexType>    
    
</xs:schema>
