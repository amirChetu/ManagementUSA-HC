      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="controllerGroupId" type="ControllerGroupId"/>
          <xs:element name="controller" type="HostingNEName"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterGetGatewayControllerListRequest">
    <xs:annotation>
      <xs:documentation>
        Get list of gateway controller entries.
