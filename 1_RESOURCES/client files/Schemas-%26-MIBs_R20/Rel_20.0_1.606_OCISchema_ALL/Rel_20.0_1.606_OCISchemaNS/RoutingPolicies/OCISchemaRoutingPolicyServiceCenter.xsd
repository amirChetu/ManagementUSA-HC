<xs:schema xmlns:xs             = "http://www.w3.org/2001/XMLSchema"
           xmlns:core           = "C"
           xmlns                = ""
           attributeFormDefault = "qualified"
           elementFormDefault   = "qualified">
  
  <xs:import namespace      = "C"
             schemaLocation = "../OCISchemaBASE.xsd"/>
  
  <xs:include schemaLocation="../OCISchemaDataTypes.xsd"/>
  <xs:include schemaLocation="../OCISchemaDataTypesNS.xsd"/>
  <xs:include schemaLocation="../OCISchemaSearchCriteriaNS.xsd"/>
  
  <!-- ********************************************************************************** -->
  <!-- S E R V I C E   C E N T E R    R E Q U E S T S  A N D  R E S P O N S E S           -->
  <!-- ********************************************************************************** -->
  <!--
  Requests and responses are listed here in alphabetical order.
  The non-primitive attributes inside the commands are defined in another
  section of the schema.

  Requests in this schema file:
    RoutingPolicyServiceCenterAddInstanceRequest18    
    RoutingPolicyServiceCenterDeleteInstanceRequest    
    RoutingPolicyServiceCenterGetInstanceRequest18    
    RoutingPolicyServiceCenterModifyInstanceRequest18    
    RoutingPolicyServiceCenterAddGatewayControllerRequest    
    RoutingPolicyServiceCenterDeleteGatewayControllerRequest    
    RoutingPolicyServiceCenterGetGatewayControllerListRequest    
    RoutingPolicyServiceCenterAddRouteRequest    
    RoutingPolicyServiceCenterDeleteRouteRequest    
    RoutingPolicyServiceCenterGetRouteEntryRequest    
    RoutingPolicyServiceCenterGetRouteListRequest    
    RoutingPolicyServiceCenterModifyRouteRequest    
    RoutingPolicyServiceCenterAddTrunkGroupRequest    
    RoutingPolicyServiceCenterDeleteTrunkGroupRequest    
    RoutingPolicyServiceCenterGetTrunkGroupEntryRequest    
    RoutingPolicyServiceCenterGetTrunkGroupListRequest    
    RoutingPolicyServiceCenterModifyTrunkGroupRequest    
    
  --> 
  
  <xs:complexType name="RoutingPolicyServiceCenterAddInstanceRequest18">
    <xs:annotation>
      <xs:documentation>
        Request to add a policy instance.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="isEnabled" type="xs:boolean"/>
          <xs:element name="callType" type="CallType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="isCicRequired" type="xs:boolean"/>
          <xs:element name="useCicAlways" type="xs:boolean"/>
          <xs:element name="addCicAsCac" type="AddCicAsCacType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterDeleteInstanceRequest">
    <xs:annotation>
      <xs:documentation>
        Request to delete a policy instance.
        The response is either SuccessResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterGetInstanceRequest18">
    <xs:annotation>
      <xs:documentation>
        Get attributes of a policy instance.
        The response is either RoutingPolicyServiceCenterGetInstanceResponse18 or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterGetInstanceResponse18">
    <xs:annotation>
      <xs:documentation>
        Response to RoutingPolicyServiceCenterGetInstanceRequest18.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="isEnabled" type="xs:boolean"/>
          <xs:element name="callType" type="CallType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="isCicRequired" type="xs:boolean"/>
          <xs:element name="useCicAlways" type="xs:boolean"/>
          <xs:element name="addCicAsCac" type="AddCicAsCacType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterModifyInstanceRequest18">
    <xs:annotation>
      <xs:documentation>
        Request to modify attributes of a policy instance.
        The response is either a SuccessResponse or an ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="name" type="PolicyInstanceName"/>
          <xs:element name="isEnabled" type="xs:boolean" minOccurs="0"/>
          <xs:element name="callTypeList" type="ReplacementCallTypeList" nillable="true" minOccurs="0"/>
          <xs:element name="isCicRequired" type="xs:boolean" minOccurs="0"/>
          <xs:element name="useCicAlways" type="xs:boolean" minOccurs="0"/>
          <xs:element name="addCicAsCac" type="AddCicAsCacType" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="RoutingPolicyServiceCenterAddGatewayControllerRequest">
    <xs:annotation>
      <xs:documentation>
        Request to add a gateway controller entry.
