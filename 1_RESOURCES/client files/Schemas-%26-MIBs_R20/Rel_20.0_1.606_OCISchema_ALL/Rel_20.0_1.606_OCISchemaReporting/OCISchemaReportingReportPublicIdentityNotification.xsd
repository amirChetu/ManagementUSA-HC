  <xs:complexContent>
    <xs:extension base="OCIReportingNotification">
      <xs:sequence>
        <xs:element name="deletedUser" minOccurs="0" maxOccurs="unbounded">
            
          <xs:complexType>
              <xs:annotation>
                <xs:appinfo>
                  <bwAppInfo bwtag="_c5bc3db860684b0794edf822f3b5119f"/>
                </xs:appinfo>
              </xs:annotation>
            <xs:sequence>
              <xs:element name="userId" type="OCIReportingUserId"/>
              <xs:element name="deletedAlias" type="OCIReportingAlias" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="deletedSipURI" type="OCIReportingSipURI" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="deletedTelURI" type="OCIReportingTelURI" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="addedUser" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
              <xs:annotation>
                <xs:appinfo>
                  <bwAppInfo bwtag="_7aa8b66a5de14f329787a5a8e311bac8"/>
                </xs:appinfo>
              </xs:annotation>
            <xs:sequence>
              <xs:element name="userId" type="OCIReportingUserId"/>
              <xs:element name="addedAlias" type="OCIReportingAlias" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="addedSipURI" type="OCIReportingSipURI" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="addedTelURI" type="OCIReportingTelURI" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="modifiedUser" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
              <xs:annotation>
                <xs:appinfo>
                  <bwAppInfo bwtag="_15af3b1f964948f1802594aaed570f8d"/>
                </xs:appinfo>
              </xs:annotation>
            <xs:sequence>
              <xs:element name="userId" type="OCIReportingUserId"/>
              <xs:element name="deletedAlias" type="OCIReportingAlias" minOccurs="0" maxOccurs="unbounded"/>
              <xs:element name="addedAlias" type="OCIReportingAlias" minOccurs="0" maxOccurs="unbounded"/>
